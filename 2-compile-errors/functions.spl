// Each of the commented lines should give a compile error

f(x) :: Int -> Bool
{
    return False;
}

g(x,y) :: Int Int -> Int
{
    // Int ~ Bool
    return x < y;
}

h() :: -> Bool
{
    if( True )
    {
        // Bool ~ Int
        return 10;
    }
    else
    {
        return False;
    }
}

i(x, y) :: Int Int -> Int
{
    return x + y;
}

// too many arguments
j(x, y) :: Int -> Int
{
    return x + y;
}

// not enough arguments
k(x) :: Int Int -> Int
{
    return x;
}


main() :: -> Void
{
    // Int ~ Bool
    print(f(True));
    // Int ~ Int -> Bool
    print(f(f));
    // Int ~ Bool
    print(f(10) + 10);

    // not enough arguments
    i(10);
    // too many arguments
    i(10, 20, 30);
}
